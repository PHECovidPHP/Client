{
    "name": "phe-covid/client",
    "description": "PHE Covid API v1 client for PHP",
    "keywords": ["covid", "covid api", "covid19", "coronavirus", "Covid API", "CovidAPI", "Covid", "Covid Client", "Covid API Client", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-json": "*",
        "phe-covid/postcodes": "^1.1",
        "php-http/cache-plugin": "^1.7.1",
        "php-http/client-common": "^2.3",
        "php-http/discovery": "^1.12",
        "php-http/httplug": "^2.2",
        "psr/cache": "^1.0",
        "psr/http-client-implementation": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/polyfill-php80": "^1.17"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4.1",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PHECovid\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHECovid\\Gen\\Client\\": "gen/",
            "PHECovid\\Tests\\Client\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
